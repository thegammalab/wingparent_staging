cklinks {    list-style:none;    margin-bottom:0px;    padding: 7px 0px 3px;}.header_quicklinks li {    background-image:url('../img/quicklinks_bullet.png');    background-repeat:no-repeat;    background-position:left center;    padding-left:20px;    margin-right:17px;    float:left;}.header_quicklinks li.search_form .search_top{    background: url('../img/top_search_white.png') center no-repeat;    width:13px;    overflow: hidden;    display:block;    height:24px;    text-indent: 999999px;    float:left;}.header_quicklinks li.search_form{    margin-right: 10px;}.header_quicklinks li.search_form form{    width:0px;    overflow: hidden;    float:left;}.header_quicklinks li.search_form form div{    width:150px;}.header_quicklinks li.search_form form input[type=text]{    background: #e2d1c2;    border-radius: 4px;    border: 0;    padding: 3px 5px;    font-size: 12px;    font-style: italic;    width:150px;}.header_quicklinks li.search_form form input[type=submit]{    background: url('../img/top_search_black.png') center no-repeat;     border: none;    margin-left:-20px;    width:16px;    overflow: hidden;    text-indent: 999999px;}.header_quicklinks li:first-child {background-image:none;}.header_quicklinks li a {    font-family:"book antiqua", Arial, sans-serif;    text-decoration:none;    font-size:21px;    color:#fff;    font-style: italic;    font-weight:normal;    -webkit-transition: all 500ms ease;    -moz-transition: all 500ms ease;    -ms-transition: all 500ms ease;    -o-transition: all 500ms ease;    transition: all 500ms ease;	}.header_quicklinks li a:hover{    color: #e4b5b5;    -webkit-transition: all 500ms ease;    -moz-transition: all 500ms ease;    -ms-transition: all 500ms ease;    -o-transition: all 500ms ease;    transition: all 500ms ease;	}.header_quicklinks li.last_item a {    font-style: normal;    font-weight:bold;}/*===============================================================================                                       END Banner Quick Links Style================================================================================*//*===============================================================================                                        Banner Quick Search Style================================================================================*/.quick_search_home {    /*    left: 15px;        position: absolute;        top: 210px;*/    display: block;    margin: 34px auto;    width: 88% !important;}.quick_search_home .qs_row {    /*    z-index:33;        position:relative;        width:320px;        margin-bottom:10px;*/}.quick_search_home .qs_row input {    background-color: transparent;    background-image: url("../img/quicksearch_bg.png");    background-repeat: repeat;    border: 1px solid #c6b297;    border-radius: 4px;    color: #fff;    font-family: "clear_sansregular",Arial,sans-serif;    /*min-width: 320px;*/    padding: 5px 30px 5px 5px;    width: 100%;}.quick_search_home .qs_row input::-webkit-input-placeholder {    color: #fff;    text-align:left;    /*font-weight:bold;*/    font-size:15px;    text-transform:none;}  .quick_search_home .qs_row input:-moz-placeholder { /* Firefox 18- */    color: #fff;    /*font-weight:bold;*/    text-align:left;    font-size:15px;    text-transform:none;    opacity:1;}.quick_search_home .qs_row input::-moz-placeholder {  /* Firefox 19+ */    color: #fff;    /*font-weight:bold;*/    text-align:left;    font-size:15px;    text-transform:none;    opacity:1;}.quick_search_home .qs_row input:-ms-input-placeholder {      font-size:15px;    /*font-weight:bold;*/    color: #fff;    text-align:left;    text-transform:none;    opacity:1;}.home .datepicker {    min-width:300px;    width:100%;    max-width:300px;}.home .datepicker table, .home .datepicker thead, .home .datepicker tbody , .home .datepicker thead tr 