cf:before, .cf:after {content:""; display:table;}.cf:after {clear:both;}/*===============================================================================                                       Font Family================================================================================*/@font-face {    font-family: 'clear_sansregular';    src: url('../fonts/clearsans-regular-webfont.eot');    src: url('../fonts/clearsans-regular-webfont.eot?#iefix') format('embedded-opentype'),        url('../fonts/clearsans-regular-webfont.woff') format('woff'),        url('../fonts/clearsans-regular-webfont.ttf') format('truetype'),        url('../fonts/clearsans-regular-webfont.svg#clear_sansregular') format('svg');    font-weight: normal;    font-style: normal;}/*===============================================================================                                       End Font Family================================================================================*//*.transition {    -webkit-transition: all 1s ease-out, color 1s ease;    -moz-transition: all 1s ease-out, color 1s ease;    -o-transition: all 1s ease-out, color 1s ease;    -ms-transition: all 1s ease-out, color 1s ease;    transition: all 1s ease-out, color 1s ease;}*//*===============================================================================                                       Header Style================================================================================*/a{    color: hsl(0, 98%, 38%);}a:hover{    color: #d85050;}.container {}.header_container {}.header {    background-color:#c6b297;}.navbar-inverse {border:0px;background-color:transparent;}.navbar-default {border:0px; background-color:transparent;}.navbar-nav > li > a {}.header_top_bar {}.header_top_bar .navbar {    min-height:10px;    margin-bottom:0px;}.header_top_bar .navbar-header {}.header_top_bar .navbar-collapse {    text-align:center;}.header_top_bar .navbar-collapse .nav {    /*    display:inline-block;        float:none;*/    display:block;    float:right;}.header .container {padding:0px;}.header .container #menu-main-menu-header li:last-child{margin-right:0px;}.header .container #menu-main-menu-header li:last-child a {padding-right:0px;}.header_top_bar .navbar-collapse .nav li  {    padding-left:14px;margin-right:14px;}.header_top_bar .navbar-collapse .nav li.active  {}.header_top_bar .navbar-collapse .nav li.active a {    background-color:transparent;    color:#000;}.header_top_bar .navbar-collapse .nav li a {    font-family:"clear_sansregular", Arial,sans-serif;    font-size:15px;    padding:7px 7px;    font-weight:normal;    color:#fff;    text-decoration:none;    -webkit-transition: all 1s ease-out, color 1s ease;    -moz-transition: all 1s ease-out, color 1s ease;    -o-transition: all 1s ease-out, color 1s ease;    -ms-transition: all 1s ease-out, color 1s ease;    transition: all 1s ease-out, color 1s ease;}.header_top_bar .navbar-collapse .nav li:hover {}.header_top_bar .navbar-collapse .nav li a:hover {    color:#a53534;    -webkit-transition: all 1s ease-out, color 1s ease;    -moz-transition: all 1s ease-out, color 1s ease;    -o-transition: all 1s ease-out, color 1s ease;    -ms-transition: all 1s ease-out, color 1s ease;    transition: all 1s ease-out, color 1s ease;}/*===============================================================================                                       END Header Style================================================================================*//*===============================================================================                                       Banner Style================================================================================*/.home_banner {    /*min-height:450px;*/    min-height:750px;;    background-repeat:no-repeat;    background-position:center bottom;    background-color:transparent;    background-size: